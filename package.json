{
  "name": "computational-magic",
  "version": "1.0.0",
  "description": "tutorial showing simple math problems and different approaches leading to different solutions",
  "main": "index.js",
  "scripts": {
    "build:ts": "tsc -b ./tsconfig.build.json",
    "build": "npm run clean && npm run build:ts",
