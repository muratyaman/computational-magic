    "tests": "mocha -r ts-node/register ./tests/**/*.test.ts",
    "tests:debug": "DEBUG=1 mocha -r ts-node/register ./tests/**/*.test.ts",
    "tests:coverage": "nyc npm run tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muratyaman/computational-magic.git"
  },
  "keywords": [
    "math",
    "mathematics",
    "computation",
    "magic",
    "problems",
    "solutions",
    "tutorial"
  ],
  "author": "Haci Murat Yaman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/muratyaman/computational-magic/issues"
  },
  "homepage": "https://github.com/muratyaman/computational-magic#readme",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.3.2",
    "chai": "^4.3.4",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
